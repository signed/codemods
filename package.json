{
  "name": "codemods",
  "private": true,
  "scripts": {
    "start": "ts-node sample/consumer.ts",
    "transform": "yarn transform:default-imports && yarn transform:default-exports",
    "transform:default-imports": "jscodeshift --extensions=ts --transform src/transformers/default-to-named-import.ts sample/",
    "transform:default-exports": "jscodeshift --extensions=ts --transform src/transformers/default-to-named-export.ts sample/",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "7.8.7",
    "@types/jest": "25.1.4",
    "@types/jscodeshift": "0.6.3",
    "babel-jest": "25.1.0",
    "jest": "25.1.0",
    "jest-extended": "0.11.5",
    "jscodeshift": "0.7.0",
    "ts-node": "8.6.2",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@types/node": "13.9.1",
    "change-case": "4.1.1"
  }
}
